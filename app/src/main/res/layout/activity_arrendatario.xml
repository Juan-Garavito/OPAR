<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#021A26"
    tools:context=".ArrendatarioActivity">

    <Button
        android:id="@+id/button"
        android:layout_width="280dp"
        android:layout_height="47dp"
        android:background="@color/white"
        android:backgroundTint="@color/white"
        android:text="¿Qué Buscas Hoy?"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.095" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="315dp"
        android:layout_height="220dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.279"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="115dp"
        android:layout_height="108dp"
        app:srcCompat="@android:drawable/presence_audio_away"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="419dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="115dp"
        android:layout_height="108dp"
        app:srcCompat="@android:drawable/presence_audio_away"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="419dp" />

    <ImageButton
        android:id="@+id/imageButton4"
        android:layout_width="115dp"
        android:layout_height="108dp"
        app:srcCompat="@android:drawable/presence_audio_away"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="578dp" />

    <ImageButton
        android:id="@+id/imageButton5"
        android:layout_width="115dp"
        android:layout_height="108dp"
        app:srcCompat="@android:drawable/presence_audio_away"
        tools:layout_editor_absoluteX="230dp"
        tools:layout_editor_absoluteY="578dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Habitacion"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="65dp"
        tools:layout_editor_absoluteY="535dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Casa"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="87dp"
        tools:layout_editor_absoluteY="693dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Apartamento"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="693dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ApartaEstudio"
        android:textColor="@color/white"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="238dp"
        tools:layout_editor_absoluteY="535dp" />

</androidx.constraintlayout.widget.ConstraintLayout>